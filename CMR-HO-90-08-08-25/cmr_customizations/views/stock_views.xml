<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="stock_picking_view_for_last_serial_number" model="ir.ui.view">
        <field name="name">view.picking.form.inherit.last.serial</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='owner_id']" position="replace">
                <field name="owner_id" groups="stock.group_tracking_owner" invisible="1"/>
            </xpath>
            <xpath expr="//group[1]" position="after">
                <group string="Transport Details" invisible="stock_type not in ['inter_state','intra_state']" >
                    <group>
                        <field name="transpoter_id" readonly="state == 'done'" required="stock_picking_type  in ['regular']" options="{'no_create': True, 'no_open': True, 'no_edit':True}"/>
                        <field name="transpoter_route_id" readonly="state == 'done'"
                               domain="[('transpoter_id','=',transpoter_id)]" required="stock_picking_type  in ['regular']" options="{'no_create': True, 'no_open': True, 'no_edit':True}"/>
                        <field name="lr_number" readonly="state == 'done'" required="stock_picking_type  in ['regular']"/>
                        <field name="driver_name" readonly="state == 'done'" required="stock_picking_type  in ['regular']"/>
                    </group>
                    <group>
                        <field name="no_of_parcel" readonly="state == 'done'" required="stock_picking_type  in ['regular']"/>
                        <field name="tracking_number" readonly="state == 'done'" force_save="1"/>
                        <field name="nhcl_tracking_number" readonly="1" force_save="1"/>
                        <field name="vehicle_number" readonly="state == 'done'" required="stock_picking_type  in ['regular']"/>
                    </group>
                </group>
            </xpath>
            <xpath expr="//field[@name='partner_id']" position="attributes">
                <attribute name="options">{'no_create': True, 'no_open': True, 'no_edit':True}</attribute>
                <attribute name="readonly">purchase_id != False</attribute>
            </xpath>
            <xpath expr="//field[@name='origin']" position="after">
                <field name="nhcl_last_serial_number" readonly="1" force_save="1"
                       invisible="picking_type_code != 'incoming'"/>
                <field name="label_click_count" readonly="1" force_save="1"/>
                <field name="purchase_id" invisible="1"/>
                <field name="is_landed_cost" invisible="stock_type not in ['ho_operation','data_import','sub_contract']"
                       readonly="state == 'done'"
                       style="border-bottom: 1px solid #714B67;padding-top: 6px;margin-bottom: 10px; width:50%"
                       placeholder="    "/>
                <field name="is_landed_cost_confirm" invisible="1" force_save="1" readonly="1"/>
                <field name="is_outgoing" invisible="1" force_save="1"/>
            </xpath>
            <xpath expr="//field[@name='origin']" position="attributes">
                <attribute name="readonly">purchase_id != False</attribute>
            </xpath>
            <xpath expr="//field[@name='partner_id']" position="attributes">
                <attribute name="readonly">purchase_id != False</attribute>
            </xpath>
            <xpath expr="//button[@name='action_open_label_type']" position="replace">
                <button name="action_open_label_type" type="object" string="Print Labels" class="btn-primary"
                        invisible="label_click_count != 0 or state != 'done' or stock_type != 'ho_operation'"/>
                <button name="action_open_label_type"
                        type="object"
                        string="Print Labels"
                        class="btn-primary" invisible="label_click_count &lt; 1 or stock_type != 'ho_operation'"
                        groups="cmr_customizations.group_print_labels_access"/>
            </xpath>
            <xpath expr="//button[@name='action_open_label_type']" position="after">
                <field name="has_landed_cost" invisible="1"/>
                <button name="action_open_landed_cost" type="object" string="Landed Cost" class="btn-secondary"  invisible="state != 'done' or is_landed_cost == 'no' or has_landed_cost"/>
                <button name="print_barcodes_preview" type="object" string="Brand" class="btn-secondary"/>
                <button name="print_ready_made_barcodes_preview" type="object" string="Ready Made" class="btn-secondary"/>
                <button name="action_open_barcode_print_wizard"
                        type="object"
                        string=" CMR Print Labels"
                        class="btn-primary" invisible="stock_picking_type == 'delivery' or state != 'done'"/>
                <field name="allow_import_order" invisible="1"/>
                    <button name="%(import_move_line_action)d"
                            string="Import Products"
                            type="action"
                            class="btn-primary" invisible="allow_import_order == False or state in ['done','cancel']"/>
            </xpath>
            <xpath expr="//button[@name='button_validate']" position="attributes">
                <attribute name="invisible">is_outgoing or state == 'done'</attribute>
            </xpath>
             <xpath expr="//button[@name='action_open_label_type']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//button[@name='action_assign']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//field[@name='move_ids_without_package']/tree" position="attributes">
                <attribute name="delete">false</attribute>
            </xpath>
            <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='product_uom_qty']"
                   position="after">
                <field name="return_parent_qty" optional="hide" readonly="1" force_save="1" sum="Return Qty"/>
                <field name="return_pend_qty" optional="hide" readonly="1" force_save="1" sum="Return Pend Qty"/>
                <field name="type_product" column_invisible="parent.picking_type_code != 'incoming'"/>
                <field name="move_brand_barcode" column_invisible="parent.stock_type != 'data_import'" required="parent.stock_type == 'data_import'"/>
                <field name="move_cp" column_invisible="parent.stock_type != 'data_import'" required="parent.stock_type == 'data_import'"/>
                <field name="move_mrp" column_invisible="parent.stock_type != 'data_import'" required="parent.stock_type == 'data_import'"/>
                <field name="move_rsp" column_invisible="parent.stock_type != 'data_import'" required="parent.stock_type == 'data_import'"/>
                <field name="dummy_lot_ids" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       widget="many2many_tags" readonly="1" force_save="1" optional="hide"/>
                <field name="move_design_id" optional="hide"/>
            </xpath>
            <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='product_uom_qty']"
                   position="attributes">
                <attribute name="sum">Quantity</attribute>
            </xpath>
            <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='quantity']"
                   position="attributes">
                <attribute name="sum">Quantity</attribute>
            </xpath>
            <xpath expr="//form/header" position="inside">
                <button name="reset_product_lines" type="object" string="Reset Lines" invisible="state == 'done'"/>
            </xpath>
            <xpath expr="//form/sheet/group/group" position="inside">
                <field name="stock_barcode" readonly="state == 'done'" invisible="stock_picking_type == 'receipt'"
                       style="border: 4px solid #714B67;padding-top: 6px;margin-bottom: 10px;"
                       placeholder="Please, Scan Barcode Here......"/>
            </xpath>
            <xpath expr="//form/sheet/group/group/field[@name='location_dest_id']" position="after">
                <field name="stock_picking_type"/>
                <field name="vendor_refund" options="{'no_create': True, 'no_open': True, 'no_edit':True}" invisible="stock_picking_type != 'goods_return'" readonly="state == 'done'" force_save="1"/>
                <field name="stock_type" placeholder="Please Stock Type..." readonly="purchase_id != False"
                       required="picking_type_code == 'incoming'"/>
                <field name="stock_bill_reference" readonly="state == 'done'"/>
                <field name="dummy_stock_type" placeholder="Please Stock Type..." invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='origin']" position="after">
                <field name="scan_or_import" widget="radio" invisible="picking_type_code != 'outgoing' or nhcl_company_bool == True"/>
                <field name="nhcl_company_bool" invisible="1"/>
                <field name="verify_barcode"
                       style="border: 4px solid #714B67;padding-top: 5px;margin-bottom: 8px;" invisible="(picking_type_code != 'outgoing' or nhcl_company_bool == True) or scan_or_import == 'import'"
                       readonly="state == 'done'"/>
                <field name="verification_success" invisible="picking_type_code != 'outgoing' or nhcl_company_bool == True" widget="badge" decoration-success="verification_success == 'matched'"/>
            </xpath>
             <xpath expr="//field[@name='label_click_count']" position="after">
                <button name="action_import_excel" type="object" string="Import Excel" class="btn-primary" invisible="(picking_type_code != 'outgoing' or nhcl_company_bool == True) or scan_or_import == 'scan'"/>
            </xpath>
            <xpath expr="//notebook" position="inside">
                <page string="Transport Routes" name="tranport_routes"
                      invisible="stock_type not in ['inter_state','intra_state']">
                    <field name="transport_location_line" readonly="1" force_save="1">
                        <tree create='false'>
                            <field name="source_location_id"/>
                            <field name="destination_location_id"/>
                            <field name="distance"/>
                            <field name="transport_charges"/>
                            <field name="time_hour"/>
                            <field name="start_time"/>
                            <field name="end_time"/>
                            <field name="tracking_number"/>
                            <field name="state"/>
                        </tree>
                    </field>
                </page>
                <page string="Transport Entry" invisible="stock_type not in ['inter_state','intra_state']">
                    <field name="transport_entry_ids" readonly="1" force_save="1"/>
                </page>
                <page name="verification" string="Verification" invisible="picking_type_code != 'outgoing'">
                    <field name="stock_verification_ids">
                        <tree editable="bottom" create="false">
                            <field name="stock_product_id"
                                   options="{'no_create': True, 'no_open': True, 'no_edit':True}"/>
                            <field name="stock_serial"/>
                            <field name="stock_qty" sum="Quantity"/>
                        </tree>
                    </field>
                </page>
                <page name="barcode" string="Barcode" invisible="picking_type_code != 'outgoing'">
                    <button name="print_barcodes_stock_picking" data-hotkey="q"
                            string="Print Barcode Sheet" class="btn-success" type="object"
                            invisible="stock_picking_type == 'receipt' or state != 'done'"/>
                    <field name="stock_picking_delivery_ids">
                        <tree editable="bottom" create="false">
                            <field name="serial_no"/>
                            <field name="lr_number"/>
                            <field name="sequence"/>
                            <field name="barcode"/>

                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>

    <record id="stock_vpicktree" model="ir.ui.view">
        <field name="name">view.picking.tree.cmr</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree/header" position="inside">
                <button name="button_import_receipts" type="object" string="Import" display="always"/>
            </xpath>
            <xpath expr="//tree" position="attributes">
                <attribute name="delete">0</attribute>
            </xpath>
        </field>
    </record>

    <record id="model_stock_picking_action_open_backdate_wizard" model="ir.actions.server">
        <field name="name">Backdate Picking</field>
        <field name="model_id" ref="cmr_customizations.model_stock_picking_backdate_wiz"/>
        <field name="binding_model_id" ref="stock.model_stock_picking"/>
        <field name="groups_id" eval="[(4, ref('cmr_customizations.stock_picking_backdate_assign_group'))]"/>
        <field name="state">code</field>
        <field name="binding_view_types">list,form</field>
        <field name="code">
            action = model.change_to_backdate_wizard()
        </field>
    </record>

