    <record id="cmr_view_production_lot_form" model="ir.ui.view">
        <field name="name">view.stock.lot.categ.des</field>
        <field name="model">stock.lot</field>
        <field name="inherit_id" ref="stock.view_production_lot_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="replace">
                <field name="name" placeholder="e.g. LOT/0001/20121" readonly="1"/>
            </xpath>
            <xpath expr="//field[@name='location_id']" position="after">
                <field name="type_product" readonly="1" force_save="1"
                      options="{'no_create': True, 'no_open': True, 'no_edit':True}"/>
                <field name="segment" placeholder="Please Segment..." readonly="1" force_save="1"/>
                <field name="picking_id" readonly="1" force_save="1"
                       options="{'no_create': True, 'no_open': True, 'no_edit':True}"/>
                <field name="serial_type" readonly="1" force_save="1"/>
                <field name="nhcl_done_qty"/>
            </xpath>
            <xpath expr="//notebook" position="inside">
                <page string="Category And Description" name="category_n_description">
                    <group>
                        <group>
                            <field name="category_1" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..." readonly="1" force_save="1"/>
                            <field name="category_2" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="category_3" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="category_4" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="category_5" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="category_6" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="category_7" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select ...." readonly="1" force_save="1"/>
                            <field name="category_8" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select ...."/>

                        </group>
                        <group>
                            <field name="description_1" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..." readonly="1" force_save="1"/>
<!--                            <field name="product_aging" options="{'no_create': True, 'no_open': True, 'no_edit':True}" invisible="0"/>-->
                            <field name="description_2" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="description_3" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="description_4" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="description_5" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="description_6" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="description_7" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                            <field name="description_8" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                                   placeholder="Please Select..."/>
                        </group>
                    </group>
                </page>
                <page name="pricing" string="Pricing">
                    <group>
                        <field name="actual_cp" readonly="1" force_save="1"/>
                        <field name="cost_price" readonly="1" force_save="1"/>
                        <field name="mr_price"/>
                        <field name="rs_price" readonly="1" force_save="1"/>
                        <!--    <field name="transfer_price"/> -->
                        <!--    <field name="transfer_percent"/> -->
                        <field name="nhcl_margin_lot" readonly="1" force_save="1"/>
                        <field name="nhcl_updated_margin_lot" readonly="1" force_save="1"/>
                        <field name="shortage" readonly="1" force_save="1"/>
                    </group>
                </page>
                <page string="Product Description">
                    <group>
                        <field name="web_product"/>
                        <field name="product_description"/>
                    </group>

                </page>
            </xpath>
            <xpath expr="//field[@name='product_id']" position="attributes">
                <attribute name="options">{'no_create': True, 'no_open': True, 'no_edit':True}</attribute>
            </xpath>

        </field>
    </record>

    <record id="cmr_search_product_lot_filter" model="ir.ui.view">
        <field name="name">view.stock.lot.search.des</field>
        <field name="model">stock.lot</field>
        <field name="inherit_id" ref="stock.search_product_lot_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="ref"/>
                <field name="picking_id"/>
            </xpath>
        </field>
    </record>

    <record model="ir.ui.view" id="landed_cost_price_inherit_view">
        <field name="name">stock.landed.cost.price.form.view</field>
        <field name="model">stock.landed.cost</field>
        <field name="inherit_id" ref="stock_landed_costs.view_stock_landed_cost_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='valuation_adjustment_lines']/tree/field[@name='final_cost']" position="after">
                <field name="actual_cp_lc" readonly="1" force_save="1"/>
                <field name="nhcl_cost_price" readonly="1" force_save="1"/>
            </xpath>
            <xpath expr="//field[@name='picking_ids']" position="attributes">
                <attribute name="required">1</attribute>
                <attribute name="readonly">1</attribute>
                <attribute name="options">{'no_create': True, 'no_open': True, 'no_edit':True}</attribute>
            </xpath>
        </field>
    </record>

    <record id="stock_picking_batch_transfer_replication_inherit_ho_view" model="ir.ui.view">
        <field name="name">stock.picking.batch.transfer.replication.form.ho.inherit</field>
        <field name="model">stock.picking.batch</field>
        <field name="inherit_id" ref="stock_picking_batch.stock_picking_batch_form"/>
        <field name="arch" type="xml">
        <xpath expr="//header" position="inside">
            <button name="post_lot_serial_numbers" type="object" string="Click Me" class="btn-primary" invisible="state != 'done'"/>
        </xpath>
            <xpath expr="//field[@name='scheduled_date']" position="after">
                <field name="nhcl_receipt_validate" widget="badge" decoration-success="nhcl_receipt_validate == 'yes'" decoration-warning="nhcl_receipt_validate == 'no'"/>
                <field name="warehouse_name" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='picking_ids']" position="attributes">
            <attribute name="domain">[('partner_id.name','=', warehouse_name),('id', 'in', allowed_picking_ids)]</attribute>
        </xpath>
<!--            <xpath expr="//field[@name='picking_ids']" position="attributes">-->
<!--                <attribute name="widget">many2many_no_remove</attribute>-->
<!--            </xpath>-->
        </field>
    </record>

    <record id="view_move_line_tree_sum" model="ir.ui.view">
        <field name="name">stock.move.line.sum</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock_picking_batch.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='quantity']" position="attributes">
                <attribute name="sum">Quantity</attribute>
            </xpath>
            <xpath expr="//tree" position="attributes">
                <attribute name="delete">false</attribute>
                <attribute name="create">false</attribute>
            </xpath>
        </field>
    </record>

    <record id="view_picking_move_tree_inherited_sum" model="ir.ui.view">
        <field name="name">stock.move.sum</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock_picking_batch.view_picking_move_tree_inherited"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='quantity']" position="attributes">
                <attribute name="sum">Quantity</attribute>
            </xpath>
            <xpath expr="//field[@name='product_uom_qty']" position="attributes">
                <attribute name="sum">Quantity</attribute>
            </xpath>
        </field>
    </record>

    <record id="view_manafacturing_import_wizard_form" model="ir.ui.view">
        <field name="name">manafacturing.import.wizard</field>
        <field name="model">manafacturing.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Excel">
                <group>
                    <field name="file_type"/>
                     <field name="file_data" widget="binary" filename="file_name"/>
                     <field name="file_name"/>
                </group>
                <footer>
                    <button string="Import" type="object" name="action_import" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_manafcturing_import" model="ir.actions.act_window">
        <field name="name">Import Excel</field>
        <field name="res_model">manafacturing.import.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <record id="nhcl_view_picking_type_form" model="ir.ui.view">
        <field name="name">view.picking.form.inherit</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='sequence_code']" position="after">
                <field name="stock_picking_type" required="1"/>
            </xpath>
        </field>
    </record>

    <record id="view_stock_verification_import" model="ir.ui.view">
        <field name="name">stock.verification.import.form</field>
        <field name="model">stock.verification.import</field>
        <field name="arch" type="xml">
            <form string="Import Stock Verification">
                <group>
                    <field name="file_type"/>
                    <field name="file_data" widget="binary" filename="file_name"/>
                    <field name="file_name"/>
                </group>
                <footer>
                    <button string="Import" type="object" name="action_import" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_stock_verification_import" model="ir.actions.act_window">
        <field name="name">Import Excel</field>
        <field name="res_model">stock.verification.import</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>


    <record id="view_stock_move_tree_inherit" model="ir.ui.view">
        <field name="name">stock.move</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="after">
                <field name="family"/>
                <field name="category"/>
                <field name="class_level_id"/>
                <field name="brick"/>
            </xpath>
        </field>
    </record>

    <record id="view_stock_move_search_inherit" model="ir.ui.view">
        <field name="name">stock.move.search.inherit</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_search"/>
        <field name="arch" type="xml">
            <xpath expr="//group" position="replace">
                <group expand="0" string="Group By">
                    <filter string="Operation Type" name="groupby_picking_type_id" domain="[]"
                            context="{'group_by': 'picking_type_id'}"/>
                    <filter name="filter_by_family" string="Division" domain="[]" context="{'group_by': 'family'}"/>
                    <filter name="filter_by_category" string="Section" domain="[]" context="{'group_by': 'category'}"/>
                    <filter name="filter_by_class" string="Department" domain="[]"
                            context="{'group_by': 'class_level_id'}"/>
                    <filter name="filter_by_brick" string="Category" domain="[]" context="{'group_by': 'brick'}"/>
                    <filter string="Product" name="by_product" domain="[]" context="{'group_by': 'product_id'}"/>
                    <filter string="Picking" name="groupby_picking_id" domain="[]"
                            context="{'group_by': 'picking_id'}"/>
                    <filter string="Source Location" name="groupby_location_id" domain="[]"
                            context="{'group_by': 'location_id'}" groups="stock.group_stock_multi_locations"/>
                    <filter string="Destination Location" name="groupby_dest_location_id" domain="[]"
                            context="{'group_by': 'location_dest_id'}" groups="stock.group_stock_multi_locations"/>
                    <filter string="Status" name="status" domain="[]" context="{'group_by': 'state'}"/>
                    <filter string="Creation Date" name="groupby_create_date" domain="[]"
                            context="{'group_by': 'create_date'}" groups="base.group_no_one"/>
                    <filter string="Scheduled Date" name="groupby_date" domain="[]" context="{'group_by': 'date'}"/>
                </group>
            </xpath>
        </field>
    </record>

     <record id="stock.stock_move_action" model="ir.actions.act_window">
        <field name="context" eval="{
            'search_default_done': 1,
            'search_default_filter_by_family': 1,
            'search_default_filter_by_class': 1,
            'search_default_filter_by_brick': 1,
            'search_default_filter_by_category': 1,
            'search_default_by_product': 1,
            'search_default_groupby_picking_type_id': 1,
        }"/>

    </record>

    <record id="view_stock_move_line_tree_inherit" model="ir.ui.view">
        <field name="name">stock.move.line</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="after">
                <field name="family" force_save="1"/>
                <field name="category"/>
                <field name="class_level_id"/>
                <field name="brick"/>
            </xpath>
        </field>
    </record>

    <record id="view_stock_move_line_search_inherit" model="ir.ui.view">
        <field name="name">stock.move.line.search.inherit</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.stock_move_line_view_search"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='groupby_product_id']" position="before">
                 <filter name="filter_by_picking_type_id" string="Operation Type" domain="[]" context="{'group_by': 'picking_type_id'}"/>
                 <filter name="filter_by_family" string="Division" domain="[]" context="{'group_by': 'family'}"/>
                 <filter name="filter_by_category" string="Section" domain="[]" context="{'group_by': 'category'}"/>
                 <filter name="filter_by_class" string="Department" domain="[]"
                            context="{'group_by': 'class_level_id'}"/>
                 <filter name="filter_by_brick" string="Category" domain="[]" context="{'group_by': 'brick'}"/>
            </xpath>
        </field>
    </record>

    <record id="stock.stock_move_line_action" model="ir.actions.act_window">
        <field name="context" eval="{
            'search_default_done': 1,
            'search_default_filter_by_family': 1,
            'search_default_filter_by_class': 1,
            'search_default_filter_by_brick': 1,
            'search_default_filter_by_category': 1,
            'search_default_by_product': 1,
            'search_default_filter_by_picking_type_id': 1,
            'search_default_groupby_product_id': 1,
        }"/>

    </record>


