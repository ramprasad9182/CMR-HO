<odoo>
    <data>
        <record id="seq_nhcl_store_id" model="ir.sequence">
            <field name="name">NHCL Store ID Sequence</field>
            <field name="code">nhcl.ho.store.master</field>
            <field name="prefix">STORE-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="ir_sequence_replication_log" model="ir.sequence">
            <field name="name">Replication Log</field>
            <field name="code">nhcl.replication.log</field>
            <field name="prefix">NR-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="ir_sequence_old_store_replication_log" model="ir.sequence">
            <field name="name">Old Store Replication Log</field>
            <field name="code">nhcl.old.store.replication.log</field>
             <field name="prefix">OR-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="ir_sequence_transaction_replication_log" model="ir.sequence">
            <field name="name">Transaction Replication Log</field>
            <field name="code">nhcl.transaction.replication.log</field>
           <field name="prefix">TR-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="ir_sequence_store_replication_log" model="ir.sequence">
            <field name="name">Store Replication Log</field>
            <field name="code">nhcl.store.replication.log</field>
           <field name="prefix">S-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="ir_sequence_job_initiated_status_log" model="ir.sequence">
            <field name="name">Job Initiated Status Log</field>
            <field name="code">nhcl.initiated.status.log</field>
           <field name="prefix">Job-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>