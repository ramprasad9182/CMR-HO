<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="nhcl_bulk_account_view" model="ir.ui.view">
        <field name="name">Bulk Account</field>
        <field name="model">nhcl.bulk.account</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="nhcl_selected_ids" options="{'no_create': True, 'no_open': True, 'no_edit':True}" widget="many2many_tags" readonly="1" invisible="1"/>
                    </group>
                    <notebook>
                        <page name="stores" string="Stores">
                            <field name="nhcl_account_line_id">
                                <tree editable="bottom" create="0" delete="0">
                                    <field name="store_id" options="{'no_create': True, 'no_open': True, 'no_edit':True}"/>
                                    <field name="is_required"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
                <footer>
                    <button string="Replicate" name="button_replicate_account" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="server_nhcl_bulk_account_view" model="ir.actions.server">
        <field name="name">Bulk Process</field>
        <field name="model_id" ref="account.model_account_account"/>
        <field name="binding_model_id" ref="account.model_account_account"/>
        <field name="state">code</field>
        <field name="code">action = records.get_account_stores()</field>
    </record>
    
