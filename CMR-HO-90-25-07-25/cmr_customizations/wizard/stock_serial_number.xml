<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_assign_serial_numbers_production_inherited" model="ir.ui.view">
        <field name="name">mrp_assign_serial_numbers_inherited</field>
        <field name="model">stock.assign.serial</field>
        <field name="inherit_id" ref="mrp.view_assign_serial_numbers_production"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='next_serial_number']" position="replace">
                <field name="nhcl_last_serial_number" string="Last Serial Number"/>
            </xpath>
        </field>
    </record>
</odoo>